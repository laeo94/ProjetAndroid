<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.projetmobile">
    <supports-screens
        android:largeScreens="false"
        android:normalScreens="false"
        android:smallScreens="false"
        android:anyDensity="false" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/account"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ListParticipateActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AccountHomeActivity" />
        <activity android:name=".AccountListingActivity" />
        <activity android:name=".AccountUpdateOrDeleteActivity" />
        <activity android:name=".AddUserActivity" />
        <activity android:name=".AddAccountActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".DepenseActivity"></activity>
    </application>

</manifest>